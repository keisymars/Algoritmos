#include <stdio.h>
#include <stdlib.h>

int main()
{
    int numero1, numero2, resultado;
    //numero 1
    printf("Digite o primeiro numero\n");
    scanf("%d", &numero1);

    //numero2
    printf("Digite o segundo numero\n");
    scanf("%d", &numero2);

    resultado= numero1+numero2;

    printf("o resultado e igual a = %d\n", resultado);

    return 0;
}
